{
  "name": "canvass",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "canvass-platform": "nx serve canvass-platform --host=0.0.0.0",
    "start": "nx serve",
    "build": "nx build",
    "test": "TZ=UTC nx affected --target=test --runInBand --maxWorkers=2",
    "test:all": "TZ=UTC nx run-many --target=test --runInBand --maxWorkers=2 --parallel=3",
    "lint": "nx run-many --target=lint --verbose --parallel=3",
    "prepare": "husky install",
    "gen:theme-typings": "chakra-cli tokens libs/components/src/theme/index.ts --strict-component-types",
    "postinstall": "npm run gen:theme-typings",
    "storybook": "nx run components:storybook",
    "storybook-solution-navigator": "nx run features-solution-navigator:storybook",
    "build-storybook": "nx run components:build-storybook --webpack-stats-json",
    "chromatic": "chromatic --exit-zero-on-changes --project-token=$CHROMATIC_PROJECT_TOKEN",
    "typecheck": "nx affected --target=tsCheck --base=main",
    "start:proxy": "npm run start & nx run proxy-api:serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "nx affected --target=lint --all",
      "nx format:write --base"
    ]
  },
  "jest-junit": {
    "classNameTemplate": "{classname} > {title}",
    "titleTemplate": "{classname} > {title}",
    "ancestorSeparator": " â€º "
  },
  "private": true,
